      "        child: Text(",
      "          label,",
      "          style: const TextStyle(color: Colors.white, fontSize: 20),",
      "          overflow: TextOverflow.clip,",
      "          softWrap: false,",
      "        ),",
      "      ),",
      "    );",
      "  }",
      "}",
      "",
      "enum CardInfo {",
      "  camera('Cameras', Icons.video_call, Color(0xff2354C7), Color(0xffECEFFD)),",
