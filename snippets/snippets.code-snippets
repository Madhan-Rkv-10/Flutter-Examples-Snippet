      "",
      "  @override",
      "  State<AnimatedAlignExample> createState() => _AnimatedAlignExampleState();",
      "}",
      "",
      "class _AnimatedAlignExampleState extends State<AnimatedAlignExample> {",
      "  bool selected = false;",
      "",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return GestureDetector(",
      "      onTap: () {",
      "        setState(() {",
