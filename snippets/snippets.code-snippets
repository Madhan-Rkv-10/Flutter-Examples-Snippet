      "class LinkedLabelRadio extends StatelessWidget {",
      "  const LinkedLabelRadio({",
      "    super.key,",
      "    required this.label,",
      "    required this.padding,",
      "    required this.value,",
      "  });",
      "",
      "  final String label;",
      "  final EdgeInsets padding;",
      "  final bool value;",
      "",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Padding(",
      "      padding: padding,",
      "      child: Row(",
      "        children: <Widget>[",
