  "Flutter TextField1 Example": {
    "scope": "dart",
    "prefix": "flutter-TextField1",
    "body": [
      "import 'package:flutter/material.dart';",
      "",
      "/// Flutter code sample for [TextField].",
      "",
      "void main() => runApp(const TextFieldExampleApp());",
