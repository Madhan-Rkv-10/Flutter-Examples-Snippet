      "                      );",
      "                    },",
      "                    childCount: 50,",
      "                    semanticIndexOffset: 56,",
      "                  ),",
      "                ),",
      "              ),",
      "            ),",
      "            SliverEnsureSemantics(",
      "              sliver: SliverToBoxAdapter(",
      "                child: IndexedSemantics(",
      "                  index: 107,",
      "                  child: Semantics(link: true, child: const Text('Link #3')),",
      "                ),",
      "              ),",
      "            ),",
      "          ],",
      "        ),",
      "      ),",
      "    );",
      "  }",
