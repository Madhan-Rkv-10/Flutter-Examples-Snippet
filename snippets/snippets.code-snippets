      "import 'package:flutter/material.dart';",
      "",
      "/// Flutter code sample for [TabBar].",
      "",
      "void main() => runApp(const TabBarApp());",
      "",
      "class TabBarApp extends StatelessWidget {",
      "  const TabBarApp({super.key});",
