      "          color: Colors.blue,",
      "          icon: child!,",
      "          onPressed: () {",
      "            setState(() {",
      "              _targetValue = _targetValue == 24.0 ? 48.0 : 24.0;",
      "            });",
      "          },",
      "        );",
      "      },",
      "      child: const Icon(Icons.aspect_ratio),",
      "    );",
      "  }",
