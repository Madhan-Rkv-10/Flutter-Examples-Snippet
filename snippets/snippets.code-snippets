      "              ToggleButtons(",
      "                direction: vertical ? Axis.vertical : Axis.horizontal,",
      "                onPressed: (int index) {",
      "                  setState(() {",
      "                    // The button that is tapped is set to true, and the others to false.",
      "                    for (int i = 0; i < _selectedWeather.length; i++) {",
      "                      _selectedWeather[i] = i == index;",
      "                    }",
