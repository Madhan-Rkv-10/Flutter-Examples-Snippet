      "  );",
      "",
      "  @override",
      "  void initState() {",
      "    super.initState();",
      "    // On web, disable the browser's context menu since this example uses a custom",
      "    // Flutter-rendered context menu.",
      "    if (kIsWeb) {",
      "      BrowserContextMenu.disableContextMenu();",
      "    }",
      "  }",
      "",
      "  @override",
